{
  "type": "Control",
  "scope": "#/properties/exampleRadioEnum",
  "options": {
    "format": "radio",
    "vuetify": {
      "v-radio-group": {
        "prepend-icon": "mdi-palette",
        "inline": true
      },
      "v-radio": {
        "Blue": {
          "color": "blue"
        },
        "Red": {
          "color": "red"
        },
        "Green": {
          "color": "green"
        }
      }
    }
  }
}
