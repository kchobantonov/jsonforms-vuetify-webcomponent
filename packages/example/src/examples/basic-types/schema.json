{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "aNull": {
      "type": "null"
    },
    "aBoolean": {
      "type": "boolean"
    },
    "aNumber": {
      "type": "number"
    },
    "anInteger": {
      "type": "integer"
    },
    "aString": {
      "type": "string"
    },
    "anArray": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "anObject": {
      "type": "object",
      "properties": {
        "nestedKey": {
          "type": "number"
        }
      },
      "required": ["nestedKey"]
    }
  }
}
