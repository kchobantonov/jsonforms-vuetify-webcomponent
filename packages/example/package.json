    "@jsonforms/vue": "3.5.0-beta.0",
    "@jsonforms/vue-vuetify": "3.5.0-beta.0",
    "@vue/compiler-dom": "^3.4.21",
    "@vueuse/core": "^10.9.0",
    "ajv": "^8.6.1",
    "ajv-errors": "^3.0.0",
    "ajv-i18n": "^4.2.0",
    "ajv-keywords": "^5.1.0",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.6",
    "json-refs": "^3.0.15",
